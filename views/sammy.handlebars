<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <link rel="stylesheet" href="css/bootstrap.min.css">
  <link rel="stylesheet" href="css/metrodna.css">
  <link href="https://fonts.googleapis.com/css?family=Open+Sans|Roboto:300,400,500,700,900|Orbitron|Oswald:300,400,700|Lobster" rel="stylesheet">
  <link rel="stylesheet" href="css/font-awesome.min.css">
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.0/jquery.min.js"></script>
  <script src="js/bootstrap.min.js"></script>
  <link rel="stylesheet" href="css/main.css">
  <title>Dashboard</title>
  <script type="text/javascript">
    $(document).ready(function() {
      function scrollDown(el) {
        el.animate({ scrollTop: el[0].scrollHeight - el.height() + 10 }, 8000, function() { scrollUp(el) });
      };
      function scrollUp(el) {
        el.animate({ scrollTop: 10 }, 8000, function() { scrollDown(el); });
      };
      scrollDown($(".transports-container"));
    });
  </script>
</head>
<body>
  <div class="col-xs-12 np header"></div>
  <div class="col-lg-9 col-md-9 col-xs-12 np left-block">
    <div class="col-lg-12 col-md-12 col-xs-12 np voisins">
      <div class="container-custom">
        <div class="title col-xs-12">
          <h1 class="col-xs-9 col-xs-push-3">ANNONCES</h1>
        </div>


        {{#each messages}}

        {{#if this.syndic }}
        <div class="block-message syndic col-xs-12">
        {{else}}
        <div class="block-message col-xs-12">
        {{/if}}

          <div class="logo-user col-xs-3">
            <div class="col-xs-4 np">
              {{#if this.syndic }}
              <img class="img-responsive" src="images/Syndic.png" alt="logo user">
              {{else}}
              <img class="img-responsive" src="images/Homme.png" alt="logo user">
              {{/if}}
            </div>
            <div class="col-xs-8">
              <p class="info-author">{{this.username}}</p>
              <p class="info-date">{{this.date}}</p>
              <p class="info-number">06 11 22 33 44</p>
            </div>
          </div>

          <div class="col-xs-9 np">
            <div class="col-xs-12 np">
              <p class="message-content">{{this.message}}</p>
            </div>
          </div>
        </div>

        {{/each}}
      </div>
    </div>

    <div class="col-lg-4 col-md-4 col-xs-12 np pub">
