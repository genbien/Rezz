<div class="container-fluid">
	<hr />
	<div class="list-group">
	{{#each schedules}}
		<li class="list-group-item">
			<span class="list-group-item-heading">
				<strong>{{this.type}} {{this.line}}</strong> - {{this.dest}}
			</span>
			<p>
				{{#each this.times}}
					<span class="label label-primary">{{this}}</span>
				{{/each}}
			</p>
		</li>
	{{/each}}
	</div>
</div>
